{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "baseUrl": "./",
    "paths": {
      "@/*": ["*"]
    },
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules", "../../archive/samples"]
}
